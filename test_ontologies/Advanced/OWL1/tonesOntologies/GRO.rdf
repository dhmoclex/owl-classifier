        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#MicroRNA"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#RibosomalRNA"/>
        <synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mRNA</synonym>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MetabolicPathway -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#MetabolicPathway">
        <rdfs:label>metabolic pathway</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#IntraCellularProcess"/>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Pathway"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasParticipant"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GO:0008152 metabolic process</reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Processes that cause many of the chemical changes in living organisms, including anabolism and catabolism. Metabolic processes typically transform small molecules, but also include macromolecular processes such as DNA repair and replication, and protein synthesis and degradation. [GO:0008152]</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Methylation -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Methylation">
        <rdfs:label>methylation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#ModificationOfMolecularEntity"/>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GO:0032259 methylation</reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The process by which a methyl group is covalently attached to a molecule. [GO:0032259]</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MicroRNA -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#MicroRNA">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">miRNA</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#RNAMolecule"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#RibosomalRNA"/>
        <synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">microRNA</synonym>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Microorganism -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Microorganism">
