        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IMR:0001657 carbohydrate</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#CatalyticActivity -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#CatalyticActivity">
