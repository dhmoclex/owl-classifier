        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Process"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A permanent change in the DNA sequence of a gene that alters the genetic message carried by that gene.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#NegativePolarity -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#NegativePolarity">
        <rdfs:label>negative polarity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Polarity"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#NegativeRegulation -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#NegativeRegulation">
