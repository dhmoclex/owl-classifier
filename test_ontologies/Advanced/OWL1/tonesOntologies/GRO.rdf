    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#OrganismalProcess -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#OrganismalProcess">
        <rdfs:label>organismal process</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Process"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasAgent"/>
                                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Organism"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Organism"/>
                                    </owl:Restriction>
                                </owl:unionOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#locatedIn"/>
                                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Organism"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#PhysiologicalProcess"/>
    </owl:Class>
    


