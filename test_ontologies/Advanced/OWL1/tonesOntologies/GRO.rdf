        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The attachment of a cell, either to another cell or to an underlying substrate such as the extracellular matrix, via cell adhesion molecules. [GO:0007155]</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#CellCommunication -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#CellCommunication">
        <rdfs:label>cell communication</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#CellularProcess"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasParticipant"/>
