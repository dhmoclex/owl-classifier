                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasAgent"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


