        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SO:0005836 regulatory region</reference>
        <synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">regulatory DNA</synonym>
    </owl:Class>
    


