                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasQuality"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PositivePolarity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any process that activates or increases the frequency, rate or extent of a biological process, function or phenomenon.</definition>
        <synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UpRegulation</synonym>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#PositiveRegulationOfGeneExpression -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#PositiveRegulationOfGeneExpression">
