            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A ligand-dependent receptor found in the nucleus of the cell. [GO:0004879]</definition>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GO:0004879 ligand-dependent nuclear receptor activity</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#NucleicAcid -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#NucleicAcid">
