    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://protege.stanford.edu/plugins/owl/protege#allowedParent -->

    <owl:DatatypeProperty rdf:about="http://protege.stanford.edu/plugins/owl/protege#allowedParent"/>
    


    <!-- http://protege.stanford.edu/plugins/owl/protege#defaultLanguage -->

    <owl:DatatypeProperty rdf:about="http://protege.stanford.edu/plugins/owl/protege#defaultLanguage"/>
    


    <!-- http://protege.stanford.edu/plugins/owl/protege#todoProperty -->

    <owl:DatatypeProperty rdf:about="http://protege.stanford.edu/plugins/owl/protege#todoProperty">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#definition -->

    <owl:DatatypeProperty rdf:about="http://www.bootstrep.eu/ontology/GRO#definition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GRO definition explaining the meaning of a class or property. If the definition was taken from an external resource a reference is given in brackets.</definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#reference -->

    <owl:DatatypeProperty rdf:about="http://www.bootstrep.eu/ontology/GRO#reference">
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference to a similar term in an external database or terminological resource.</definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#synonym -->

    <owl:DatatypeProperty rdf:about="http://www.bootstrep.eu/ontology/GRO#synonym">
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An exact synonyms of the given ontology term.</definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/2005/08/07/xsp.owl#base -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/2005/08/07/xsp.owl#base">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.bootstrep.eu/ontology/GRO#AP2EREBPRelatedDomain -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#AP2EREBPRelatedDomain">
        <rdfs:label>AP2-EREBP-related domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#DNABindingDomainOfTF"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#AP2_EREBPRelatedTF"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A domain consisting of the AP2 (APETALA2) and EREBPs (ethylene-responsive element binding proteins) related DNA-binding sequence.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#AP2_EREBPRelatedTF -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#AP2_EREBPRelatedTF">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AP2-EREBP related transcription factor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#EnhancerBindingTF"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#AP2EREBPRelatedDomain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A transcription factor containing the AP2 (APETALA2) and EREBPs (ethylene-responsive element binding proteins) related DNA-binding domain, which plays a variety of roles throughout the plant life cycle.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Acetylation -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Acetylation">
        <rdfs:label>acetylation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#ModificationOfMolecularEntity"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Activation -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Activation">
        <rdfs:label>activation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#RegulatoryProcess"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasQuality"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PositivePolarity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#ActivationOfPolymeraseActivity -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#ActivationOfPolymeraseActivity">
        <rdfs:label>activation of polymerase</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Activation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PolymeraseActivity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PolymeraseActivity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#ActivationOfProcess -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#ActivationOfProcess">
        <rdfs:label>activation of process</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Activation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Process"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Process"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#ActivationOfProteinFunction -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#ActivationOfProteinFunction">
        <rdfs:label>activation of protein</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Activation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#ProteinFunction"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#ProteinFunction"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#ActivationOfTranscriptionRegulatorActivity -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#ActivationOfTranscriptionRegulatorActivity">
        <rdfs:label>activation of transcription regulator activity</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Activation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#TranscriptionRegulatorActivity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPatient"/>
                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#TranscriptionRegulatorActivity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any process that activates or increases the frequency, rate or extent of activity of a transcription regulator, any factor plays a role in regulating transcription; may bind a promoter or enhancer DNA sequence or interact with a DNA-binding transcription factor (modified from [GO:0051091, GO:0030528])</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Agonist -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Agonist">
        <rdfs:label>agonist</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Ligand"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#AminoAcid -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#AminoAcid">
        <rdfs:label>amino acid</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Chemical"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Carbohydrate"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Gas"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#InositolPhosphate"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Ion"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Lipid"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Nucleoside"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#SecondMessenger"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Vitamin"/>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CHEBI:33709 amino acids</reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carboxylic acids containing one or more amino groups. [CHEBI:33709]</definition>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IMR:0001695 amino acid</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Antagonist -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Antagonist">
        <rdfs:label>antagonist</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Ligand"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An agent that opposes the action or reduces the physiological activity of another.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Apoenzyme -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Apoenzyme">
        <rdfs:label>apoenzyme</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Protein"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Archaebacterium -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Archaebacterium">
        <rdfs:label>archaebacterium</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Prokaryote"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Bacterium"/>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCBI Taxonomy ID: 2157  Archaea</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#ArchitecturalProteins -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#ArchitecturalProteins">
        <rdfs:label>architectural proteins</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Protein"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proteins that bend DNA.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Atom -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Atom">
        <rdfs:label>atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Attenuator -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Attenuator">
        <rdfs:label>attenuator</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#DNARegion"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A sequence segment located within the five prime end of an mRNA that causes premature termination of translation. [SO:0000140]</definition>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SO:0000140 attenuator</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#BacterialRNAPolymerasePromoter -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#BacterialRNAPolymerasePromoter">
        <rdfs:label>bacterial RNA polymerase promoter</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Promoter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#fromSpecies"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#Bacterium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A DNA sequence to which bacterial RNA polymerase binds, to begin transcription. [SO:0000613]</definition>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SO:0000613 bacterial RNApol promoter</reference>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Bacteriophage -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#Bacteriophage">
        <rdfs:label>bacteriophage</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Virus"/>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCBI Taxonomy ID: 38018 unidentified phage</reference>
    </owl:Class>
    


