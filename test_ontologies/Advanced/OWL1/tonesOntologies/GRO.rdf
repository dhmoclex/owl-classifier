        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#ThreeDimensionalMolecularStructure"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MessengerRNA -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#MessengerRNA">
