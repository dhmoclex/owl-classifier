        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#SecondMessenger"/>
        <owl:disjointWith rdf:resource="http://www.bootstrep.eu/ontology/GRO#Vitamin"/>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CHEBI: 33838 nucleosides</reference>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IMR:0001697 nucleoside</reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ribosyl or deoxyribosyl derivatives of the pyrimidine or purine bases adenine, guanine, xanthine, thymine, cytosine, and uracil. [CHEBI: 33838]</definition>
    </owl:Class>
    


