                <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PhysicalContinuant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasAgent"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#PhysicalContinuant"/>
            </owl:Restriction>
