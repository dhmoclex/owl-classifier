        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#DNARegion"/>
        <synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CpG island</synonym>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Regions of a few hundred to a few thousand bases in vertebrate genomes that are relatively GC and CpG rich; they are typically unmethylated and often found near the 5&#39; ends of genes. [SO:0000307]</definition>
        <reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SO:0000307 CpG_island</reference>
    </owl:Class>
    


