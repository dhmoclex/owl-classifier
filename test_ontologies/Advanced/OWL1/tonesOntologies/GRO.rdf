        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#DNARegion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#patientOf"/>
