        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.bootstrep.eu/ontology/GRO#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#WRKY"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A transcription factor containing WRKY domain.</definition>
    </owl:Class>
    


