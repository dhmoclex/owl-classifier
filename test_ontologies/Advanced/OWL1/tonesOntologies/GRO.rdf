    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MutantGene -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#MutantGene">
        <rdfs:label>mutant gene</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Gene"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A gene that has changed (e.g. lost, gained, or exchanged some of the material it received from its parent), affecting the normal transmission and expression of a trait.</definition>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MutatedProtein -->

    <owl:Class rdf:about="http://www.bootstrep.eu/ontology/GRO#MutatedProtein">
        <rdfs:label>mutated protein</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bootstrep.eu/ontology/GRO#Protein"/>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#Mutation -->

