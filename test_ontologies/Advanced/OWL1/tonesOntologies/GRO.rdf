                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Fungus"/>
                    <rdf:Description rdf:about="http://www.bootstrep.eu/ontology/GRO#Protist"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


