                        <owl:allValuesFrom rdf:resource="http://www.bootstrep.eu/ontology/GRO#MolecularEntity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.bootstrep.eu/ontology/GRO#MolecularStructure -->

