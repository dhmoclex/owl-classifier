                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#FishTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#FishTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#FishTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#PizzaTopping"/>
        <owl:disjointWith rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MeatTopping"/>
        <owl:disjointWith rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#VegetarianTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Gas -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Gas">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MaterialState"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#IceCream -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#IceCream">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#PizzaDomainConcept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#FishTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">All IceCreams must have at least 1 topping.
Any individual that has a topping must be a Pizza.
Therefore all IceCreams must be Pizzas.
Icecream is inferred to be a subclass of Pizza.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Liquid -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Liquid">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MaterialState"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#LiquidSolidIceCream -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#LiquidSolidIceCream">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#IceCream"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasMaterialState"/>
                        <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Liquid"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasMaterialState"/>
                        <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Solid"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class that uses a functional property (hasMaterialState) twice. This is not inconsistent unless MaterialStates have been declared disjoint</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MaterialState -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MaterialState">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The MaterialStates should be disjoint, but this has been left out - see LiquidSolidIceCream</rdfs:comment>
    </owl:Class>
    


