     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:brokenPizza="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#">
    <owl:Ontology rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl">
        <protege:defaultLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></protege:defaultLanguage>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p&gt;A set of classes used to demonstrate common errors made while modelling, including: &lt;ol&gt; &lt;li&gt;Over-constraining domains on properties&lt;/li&gt; &lt;li&gt;Misinterpretation of functional properties&lt;/li&gt; &lt;li&gt;Descriptions of VegetarianPizza that:&lt;ol&gt; 								 &lt;li&gt;can be trivially satisfied&lt;/li&gt; 								 &lt;li&gt;can subsume unclosed pizzas&lt;/li&gt; 								 &lt;li&gt;use restrictions that do not mean anything&lt;/li&gt; 								 &lt;li&gt;use intersection incorrectly&lt;/li&gt; 								 &lt;li&gt;use inappropriate disjoints&lt;/li&gt; 								 &lt;/ol&gt; &lt;/li&gt; &lt;/ol&gt; VegePizzas have been left as primitives to simplify viewing the classification. Convert each of these, one at a time or in small groups, to a defined class (using the right-click menu) before classifying. &lt;/p&gt;</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">version 1.0</owl:versionInfo>
        <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/protege"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#versionInfo"/>
    <owl:AnnotationProperty rdf:about="http://protege.stanford.edu/plugins/owl/protege#defaultLanguage"/>
    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasMaterialState -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasMaterialState">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#hasTopping">
        <rdfs:domain rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


