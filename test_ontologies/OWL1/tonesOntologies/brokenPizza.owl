                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/brokenPizza/2005/07/06/brokenPizza.owl#MeatTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


